{
  "name": "wechaty-freshdesk",
  "version": "0.6.9",
  "description": "Wechaty Freshdesk Plugin helps Freshdesk unifies conversations from WeChat, and helps you resolve issues across channels effortlessly!",
  "main": "dist/src/mod.js",
  "typings": "dist/src/mod.d.ts",
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "pack": "npm pack",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tsc --noEmit",
    "example": "ts-node examples/ding-dong-bot.ts",
    "start": "npm run example",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\"",
    "lint:es": "eslint --ignore-pattern tests/fixtures/ '{bin,examples,scripts,src,tests}/**/*.ts'"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "smee-client": "^1.1.0",
    "unirest": "^0.6.0",
    "wechaty-plugin-contrib": "^0.14.7"
  },
  "devDependencies": {
    "@chatie/eslint-config": "^0.12.1",
    "@chatie/git-scripts": "^0.6.2",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.10.1",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "pkg-jq": "^0.2.4",
    "shx": "^0.3.2",
    "tstest": "^0.4.10",
    "wechaty": "^0.62.3",
    "wechaty-puppet-mock": "^0.28.2"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  },
  "publishConfig": {
    "tag": "next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wechaty/wechaty-freshdesk.git"
  },
  "keywords": [
    "wechaty",
    "freshdesk",
    "plugin"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wechaty/wechaty-freshdesk/issues"
  },
  "homepage": "https://github.com/wechaty/wechaty-freshdesk#readme"
}
